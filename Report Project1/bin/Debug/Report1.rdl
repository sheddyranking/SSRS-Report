  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@actor_name">
            <Value>=Parameters!actor_name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	tblActor.ActorName,
	tblFilm.FilmName,
	tblCast.CastCharacterName
FROM tblActor 
	INNER JOIN tblCast 
	ON tblActor.ActorID = tblCast.CastActorID 
	INNER JOIN tblFilm 
	ON tblCast.CastFilmID = tblFilm.FilmID
WHERE tblActor.ActorName IN (@actor_name)
ORDER BY tblActor.ActorName, tblFilm.FilmName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ActorName">
          <DataField>ActorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CastCharacterName">
          <DataField>CastCharacterName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Actor_names">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
	DISTINCT(tblActor.ActorName)
FROM tblActor ;
	 </CommandText>
      </Query>
      <Fields>
        <Field Name="ActorName">
